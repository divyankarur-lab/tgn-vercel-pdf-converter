{
  "functions": {
    "api/generate-pdf.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
  },
  "buildCommand": "npm install",
  "builds": [
    {
      "src": "api/generate-pdf.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "node_modules/@sparticuz/chromium/bin/*"
      }
    }
  ]
}